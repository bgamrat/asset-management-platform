{% block title %}{{ 'common.test'|trans }}{% endblock %}
{% extends '/admin.base.html.twig' %}

{% block admin_content %}

    <h1>{{ block ('title') }}</h1>

    <button id="test-btn">Test</button>

    {#% javascripts '@test_js' %}
    <script data-dojo-config="async:true"
    src="{{ asset_url }}"></script>
    {% endjavascripts %#}



{% endblock %}  
{% block page_js %}
    {% if app.environment == 'prod' %}
        <script src="/release/app/test.js"></script>
    {% endif %}
    <script>
        require(["app/test",
            "dojo/domReady!"], function (test) {
            test.run();
        });
    </script>
{% endblock %}
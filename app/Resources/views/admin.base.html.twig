<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% stylesheets '@dojo_dijit_css'
            '@dgrid_css'
            '@main_css'
            '@admin_css'
            filter='cssrewrite' output='css/compiled/app.css' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
    {% block stylesheets %}{% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body class="tundra">
    Admin Base
    <h3>Menu Demo</h3>
    <div id="mainMenu"></div>
    <p>Last selected: <span id="lastSelected">none</span></p>
    <p>Base</p>
{% block body %}
{% block admin_user_content %}{% endblock %}
{% endblock %}

{% include 'parts/page_footer_script.html.twig' %}
{% javascripts
    '@lib_js'
    '@admin_js'
%}
<script src="{{ asset_url }}"></script>
{% endjavascripts %}
{% block javascripts %}{% endblock %}
</body>
</html>

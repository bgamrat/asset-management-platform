{
    "name": "bgamrat/dev",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "files": [
            "app/AppKernel.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "sitepen/dojo",
                "version": "1.13.0",
                "dist": {
                    "type": "tar",
                    "url": "https://download.dojotoolkit.org/release-1.13.0/dojo-release-1.13.0-src.tar.gz"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "sitepen/dgrid",
                "version": "1.2.1",
                "dist": {
                    "type": "tar",
                    "url": "https://github.com/SitePen/dgrid/archive/v1.2.1.tar.gz"
                }
            }
        }, {
            "type": "package",
            "package": {
                "name": "sitepen/dstore",
                "version": "1.1.2",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/SitePen/dstore/archive/master.zip"
                }
            }
        }, {
            "type": "package",
            "package": {
                "name": "kriszyp/put-selector",
                "version": "0.3.6",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/kriszyp/put-selector/archive/master.zip"
                }
            }
        }, {
            "type": "package",
            "package": {
                "name": "kriszyp/xstyle",
                "version": "0.3.2",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/kriszyp/xstyle/archive/master.zip"
                }
            }
        }, {
            "type": "package",
            "package": {
                "name": "persvr/rql",
                "version": "0.3.3",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/persvr/rql/archive/master.zip"
                }
            }
        }, {
            "type": "package",
            "package": {
                "name": "FortAwesome/Font-Awesome",
                "version": "4.7.0",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/FortAwesome/Font-Awesome/archive/master.zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "twbs/bootstrap",
                "version": "3.3.7",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/twbs/bootstrap/releases/download/v3.3.7/bootstrap-3.3.7-dist.zip"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "3.3.*",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^2.8",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "symfony/security": "^3.0",
        "symfony/security-bundle": "3.0",
        "symfony/assetic-bundle": "^2.7",
        "twig/extensions": "^1.3",
        "friendsofsymfony/user-bundle": "^2.0",
        "symfony/security-csrf": "^3.0",
        "symfony/asset": "^3.0",
        "kriswallsmith/assetic": "^1.3",
        "friendsofsymfony/rest-bundle": "^1.7",
        "stof/doctrine-extensions-bundle": "^1.2",
        "knplabs/knp-menu-bundle": "^2.0",
        "symfony/translation": "^3.1",
        "symfony/config": "^3.1",
        "sitepen/dojo": "^1.11",
        "sitepen/dgrid": "^1.1",
        "sitepen/dstore": "^1.1",
        "kriszyp/put-selector": "^0.3.3",
        "persvr/rql": "^0.3.3",
        "FortAwesome/Font-Awesome": "^4.7",
        "twbs/bootstrap": "^3.3",
        "kriszyp/xstyle": "^0.3.2",
        "symfony/dependency-injection": "^3.3",
        "symfony/yaml": "^3.3"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0",
        "doctrine/doctrine-fixtures-bundle": "^2.3"
    },
    "scripts": {
        "mvDojo": "cp -rf vendor/sitepen/dojo/* web/vendor/dojo; rm -rf vendor/sitepen/dojo",
        "mvDgridDstore": "cp -rf vendor/sitepen/* web/vendor/dojo; rm -rf vendor/sitepen/dgrid vendor/sitepen/dstore",
        "mvZyp": "cp -rf vendor/kriszyp/* web/vendor/dojo; rm -rf vendor/kriszyp",
        "mvRql": "cp -rf vendor/persvr/* web/vendor/dojo; rm -rf vendor/persvr",
        "mvBootstrap": "cp -rf vendor/twbs/* web/vendor; rm -rf vendor/twbs",
        "mvFontAwesome": "cp -rf vendor/FortAwesome/Font-Awesome web/vendor; cp -rf vendor/FortAwesome/Font-Awesome/fonts web/vendor; rm -rf vendor/FortAwesome",
        "rmAllJS": "rm -rf web/vendor/dojo web/vendor/bootstrap web/vendor/fonts",
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "@mvDojo",
            "@mvDgridDstore",
            "@mvZyp",
            "@mvRql",
            "@mvBootstrap",
            "@mvFontAwesome"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "@mvDojo",
            "@mvDgridDstore",
            "@mvZyp",
            "@mvRql",
            "@mvBootstrap",
            "@mvFontAwesome"
        ],
        "post-package-uninstall": [
            "@rmAllJS"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}

{
    "description": "asset-management-system",
    "name": "bgamrat/asset-management-system",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "sitepen/dojo",
                "version": "1.13.0",
                "dist": {
                    "type": "tar",
                    "url": "https://download.dojotoolkit.org/release-1.13.0/dojo-release-1.13.0-src.tar.gz"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "sitepen/dgrid",
                "version": "1.2.1",
                "dist": {
                    "type": "tar",
                    "url": "https://github.com/SitePen/dgrid/archive/v1.2.1.tar.gz"
                }
            }
        }, {
            "type": "package",
            "package": {
                "name": "sitepen/dstore",
                "version": "1.1.2",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/SitePen/dstore/archive/master.zip"
                }
            }
        }, {
            "type": "package",
            "package": {
                "name": "kriszyp/put-selector",
                "version": "0.3.6",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/kriszyp/put-selector/archive/master.zip"
                }
            }
        }, {
            "type": "package",
            "package": {
                "name": "kriszyp/xstyle",
                "version": "0.3.2",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/kriszyp/xstyle/archive/master.zip"
                }
            }
        }, {
            "type": "package",
            "package": {
                "name": "persvr/rql",
                "version": "0.3.3",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/persvr/rql/archive/master.zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "twbs/bootstrap",
                "version": "3.3.7",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/twbs/bootstrap/releases/download/v3.3.7/bootstrap-3.3.7-dist.zip"
                }
            }
        }
    ],
    "require": {
        "php": "^7.1.3",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/framework-bundle": "^4.0",
        "symfony/monolog-bundle": "^3.2",
        "sensio/framework-extra-bundle": "^5.1",
        "incenteev/composer-parameter-handler": "^2.0",
        "symfony/security-bundle": "^4.0",
        "symfony/process": "^4.0",
        "twig/extensions": "^1.3",
        "symfony/security-csrf": "^3.0",
        "symfony/asset": "^4.0",
        "friendsofsymfony/rest-bundle": "^2.2",
        "stof/doctrine-extensions-bundle": "^1.2",
        "knplabs/knp-menu-bundle": "^2.0",
        "symfony/translation": "^4.0",
        "sitepen/dojo": "^1.13",
        "sitepen/dgrid": "^1.1",
        "sitepen/dstore": "^1.1",
        "ext-iconv": "*",
        "kriszyp/put-selector": "^0.3.3",
        "symfony/expression-language": "^4.0",
        "persvr/rql": "^0.3.3",
        "FortAwesome/Font-Awesome": "^4.7",
        "symfony/serializer-pack": "*",
        "twbs/bootstrap": "^3.3",
        "kriszyp/xstyle": "^0.3.2",
        "symfony/dependency-injection": "^4.0",
        "symfony/yaml": "^4.0",
        "dunglas/doctrine-json-odm": "^0.1.1",
        "components/font-awesome": "^5.0",
        "symfony/flex": "^1.0",
        "symfony/orm-pack": "^1.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/form": "^4.0",
        "symfony/validator": "^4.0",
        "friendsofsymfony/user-bundle": "^2.1",
        "symfony/web-link": "^4.0",
        "symfony/debug-pack": "*",
        "symfony/property-info": "^4.0",
        "composer/composer": "^1.6",
        "composer/ca-bundle": "^1.1",
        "symfony/maker-bundle": "^1.4",
        "symfony/dotenv": "^4.0",
        "symfony/webpack-encore-pack": "^1.0",
        "sensiolabs/security-checker": "^4.1",
        "symfony/templating": "^4.0",
        "symfony/requirements-checker": "^1.1",
        "jms/serializer-bundle": "^2.4",
        "api-platform/api-pack": "^1.1"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "symfony/browser-kit": "^4.0",
        "symfony/css-selector": "^4.0",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/web-server-bundle": "^4.0"

    },
    "scripts": {
	"mvDojo": "mkdir -p assets/vendor/dojo; cp -rf vendor/sitepen/dojo/* assets/vendor/dojo; rm -rf vendor/sitepen/dojo",
        "mvDgridDstore": "cp -rf vendor/sitepen/* assets/vendor/dojo; rm -rf vendor/sitepen/dgrid vendor/sitepen/dstore",
        "mvZyp": "cp -rf vendor/kriszyp/* assets/vendor/dojo; rm -rf vendor/kriszyp",
        "mvRql": "cp -rf vendor/persvr/* assets/vendor/dojo; rm -rf vendor/persvr",
        "mvBootstrap": "cp -rf vendor/twbs/* public/vendor; rm -rf vendor/twbs/*",
        "mvFontAwesome": "cp -rf vendor/fortawesome/font-awesome public/vendor; rm -rf vendor/fort-awesome/*",
        "rmAllJS": "rm -rf public/vendor/dojo/* public/vendor/bootstrap/* public/vendor/fortawesome/*",
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "@mvDojo",
            "@mvDgridDstore",
            "@mvZyp",
            "@mvRql",
            "@mvBootstrap",
            "@mvFontAwesome"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "@mvDojo",
            "@mvDgridDstore",
            "@mvZyp",
            "@mvRql",
            "@mvBootstrap",
            "@mvFontAwesome"
        ],
        "post-package-uninstall": [
            "@rmAllJS"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "security-checker security:check": "script",
            "requirements-checker": "script"
        }
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
